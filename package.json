{
  "name": "multilang-autofixer",
  "displayName": " CodeMedic",
  "description": "Multi-Language Real-Time Syntax Auto-Fixer Using Tree-sitter",
  "version": "0.0.1",
  "publisher": "Aryan Shrivastav",
  "engines": {
  "vscode": "^1.104.0"
  },
  "categories": ["Programming Languages", "Linters"],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:javascript",
    "onLanguage:java",
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:go",
    "onLanguage:ruby",
    "onLanguage:rust",
    "onLanguage:php"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autoFixSyntax.start",
        "title": "Start Auto Syntax Fixer"
      },
       {
        "command": "multilang-autofixer.ShowLog ",
        "title": "AutoFix: ShowLog"
      },
       {
        "command": "multilang-autofixer.toggle ",
        "title": "AutoFix: Toggle On/off"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Multi-Language Auto-Fixer",
      "properties": {"multilangAutofix.autoapply":{
        "type": "boolean",
        "default":true ,
        "description": "Automatically apply safe fixes without confirmation."
      },
      "multilangAutofix.python.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for Python."
      },

      "multilangAutofix.javascript.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for javascript."
      },

      "multilangAutofix.Java.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for Java."
      },
      "multilangAutofix.php.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for php."
      },
      "multilangAutofix.cpp.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for Python."
      },
      "multilangAutofix.go.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for Python."
      },
      "multilangAutofix.c.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for Python."
      },
      "multilangAutofix.ruby.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for Python."
      },
      "multilangAutofix.rust.ensabled" : {
        "type" :"boolean",
        "default":true ,
        "description": "Ensabled AutoFixs for Python."
      }
    }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.18.6",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "tree-sitter-c": "^0.24.1",
    "tree-sitter-cpp": "^0.23.4",
    "tree-sitter-go": "^0.25.0",
    "tree-sitter-java": "^0.23.5",
    "tree-sitter-javascript": "^0.25.0",
    "tree-sitter-php": "^0.24.2",
    "tree-sitter-python": "^0.25.0",
    "tree-sitter-ruby": "^0.23.1",
    "tree-sitter-rust": "^0.24.0"
  }
}
